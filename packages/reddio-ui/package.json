{
  "name": "@app/reddio-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "rnstl && start-storybook -p 9009 -s public",
    "build-storybook": "rnstl && build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./.storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "apollo-boost": "^0.1.23",
    "graphql": "^14.0.2",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "open-iconic": "^1.1.1",
    "react": "^16.6.3",
    "react-apollo": "^2.3.3",
    "react-art": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-native-web": "^0.9.9",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-links": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "babel-loader": "^8.0.4",
    "react-native-storybook-loader": "^1.8.0"
  }
}
