{
  "name": "reddit-clients",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./.storybook/storyLoader.js"
    }
  },
  "scripts": {
    "format": "prettier --write '**/**/*.{js,ts,css,html,md,yaml,yml}'",
    "test:format": "prettier --check '**/**/*.{js,ts,css,html,md,yaml,yml}'",
    "test:cra": "react-scripts test",
    "test": "run-s test:*",
    "build:cra": "react-scripts build",
    "build:tsc": "tsc --project ./server",
    "build": "run-s build:*",
    "dev:cra": "BROWSER=false react-scripts start",
    "dev:now": "now --local-config .zeit/now.dev.json dev --listen 4000",
    "dev:storybook": "rnstl && start-storybook -p 9009 -s public --ci",
    "dev": "run-p dev:*",
    "storybook": "rnstl && build-storybook -s public --quiet",
    "deploy:preview": "now --local-config .zeit/now.prod.json dev --listen 4000",
    "deploy": "now --local-config .zeit/now.prod.json"
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-server-hapi": "^2.4.8",
    "axios": "^0.18.0",
    "dataloader": "^1.4.0",
    "graphql": "14.2.1",
    "hapi": "^18.1.0",
    "immer": "^2.1.4",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "open-iconic": "^1.1.1",
    "qs": "^6.7.0",
    "react": "^16.8.5",
    "react-apollo": "^2.5.2",
    "react-art": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-native-web": "^0.11.2",
    "react-player": "^1.10.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.4.0",
    "use-media": "^1.3.0"
  },
  "devDependencies": {
    "@percy/storybook": "^3.2.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/axios": "^0.14.0",
    "@types/graphql": "14.2.0",
    "@types/hapi": "18.0.1",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/node": "11.13.0",
    "fast-check": "^1.15.1",
    "now": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-native-storybook-loader": "^1.8.0",
    "typescript": "^3.4.2"
  }
}
